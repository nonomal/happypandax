{
  "name": "happypandax",
  "version": "0.0.0",
  "main": "index.js",
  "repository": "https://github.com/happypandax/happypandax",
  "author": "Twiddly <pew@pewpew.moe>",
  "license": "LGPL-3.0",
  "private": true,
  "scripts": {
    "run:tools": "yarn workspace @happypandax/tools",
    "start:tools": "yarn workspace @happypandax/tools start",
    "run:client": "yarn workspace @happypandax/client",
    "add:client": "yarn workspace @happypandax/client add",
    "remove:client": "yarn workspace @happypandax/client remove",
    "start:client": "yarn workspace @happypandax/client dev"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "rimraf": "^4.1.3"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "resolutions": {
    "node-gyp": "^9.3.0"
  },
  "packageManager": "yarn@3.3.0",
  "installConfig": {
    "hoistingLimits": "dependencies"
  }
}
